<Window x:Class="Benfinit_water.View.win_menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Benfinit_water.View"
        mc:Ignorable="d"
        Title="win_menu"
        Height="800"
        Width="1320">
    <Grid Background="#E8E8E8">

        <Grid Background="#E8E8E8"
              Height="720"
              Width="1280">
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="1"
                  Grid.Column="0"
                  Grid.RowSpan="6"
                  Grid.ColumnSpan="1"
                  HorizontalAlignment="Stretch"
                  VerticalAlignment="Stretch">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Border x:Name="home_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="0"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="home_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="0"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="home_enter"
                        MouseLeave="home_leave"
                        MouseLeftButtonDown="home_down">
                    <Image Name="home_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/home_1.png" />
                        </Image.Source>
                    </Image>
                </Border>
                <Border x:Name="quy_hoach_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="quy_hoach_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="quy_hoach_enter"
                        MouseLeave="quy_hoach_leave"
                        MouseLeftButtonDown="quy_hoach_down">
                    <Image Name="quy_hoach_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/quy_hoach_1.png" />
                        </Image.Source>
                    </Image>
                </Border>
                <Border x:Name="dap_nuoc_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="2"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="dap_nuoc_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="2"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="dap_nuoc_enter"
                        MouseLeave="dap_nuoc_leave"
                        MouseLeftButtonDown="dap_nuoc_down">
                    <Image Name="dap_nuoc_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/dap_nuoc_1.png" />
                        </Image.Source>
                    </Image>
                </Border>
                <Border x:Name="bom_nuoc_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="3"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="bom_nuoc_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="3"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="bom_nuoc_enter"
                        MouseLeave="bom_nuoc_leave"
                        MouseLeftButtonDown="bom_nuoc_down">
                    <Image Name="bom_nuoc_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/bom_nuoc_1.png" />
                        </Image.Source>
                    </Image>
                </Border>
                <Border x:Name="duong_ong_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="4"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="duong_ong_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="4"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="duong_ong_enter"
                        MouseLeave="duong_ong_leave"
                        MouseLeftButtonDown="duong_ong_down">
                    <Image Name="duong_ong_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/duong_ong_1.png" />
                        </Image.Source>
                    </Image>
                </Border>
                <Border x:Name="ban_do_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="5"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="ban_do_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="5"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="ban_do_enter"
                        MouseLeave="ban_do_leave"
                        MouseLeftButtonDown="ban_do_down">
                    <Image Name="ban_do_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/ban_do_1.png" />
                        </Image.Source>
                    </Image>
                </Border>
                <Border x:Name="he_thong_trang"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_trang}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="6"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"></Border>
                <Border x:Name="he_thong_den"
                        Background="#E8E8E8"
                        Style="{StaticResource hieu_ung_bong_den}"
                        CornerRadius="5"
                        Height="50"
                        Width="50"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Grid.Row="6"
                        Grid.Column="0"
                        Grid.RowSpan="1"
                        Grid.ColumnSpan="1"
                        MouseEnter="he_thong_enter"
                        MouseLeave="he_thong_leave"
                        MouseLeftButtonDown="he_thong_down">
                    <Image Name="he_thong_png"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center">
                        <Image.Source>
                            <BitmapImage UriSource="png/he_thong_1.png" />
                        </Image.Source>
                    </Image>
                </Border>

            </Grid>
            <Border x:Name="man_hinh_trang"
                    Style="{StaticResource hieu_ung_bong_trang}"
                    Background="#E8E8E8"
                    CornerRadius="5"
                    VerticalAlignment="Stretch"
                    HorizontalAlignment="Right"
                    Height="700"
                    Width="1280"
                    Grid.Row="0"
                    Grid.Column="0"
                    Grid.RowSpan="8"
                    Grid.ColumnSpan="18"></Border>
            <Border x:Name="man_hinh_den"
                    Style="{StaticResource hieu_ung_bong_den}"
                    Background="#E8E8E8"
                    CornerRadius="5"
                    VerticalAlignment="Stretch"
                    HorizontalAlignment="Right"
                    Height="700"
                    Width="1280"
                    Grid.Row="0"
                    Grid.Column="0"
                    Grid.RowSpan="8"
                    Grid.ColumnSpan="18">
                <Grid>
                    <ContentControl x:Name="noi_dung"
                                    Margin="0 70 0 0"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Grid.RowSpan="8"
                                    Grid.ColumnSpan="17">

                    </ContentControl>
                    <Border x:Name="menu_trang"
                            Margin="15"
                            Background="#E8E8E8"
                            Style="{StaticResource hieu_ung_bong_trang}"
                            CornerRadius="5"
                            Height="50"
                            Width="50"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Grid.Row="0"
                            Grid.Column="1"
                            Grid.RowSpan="8"
                            Grid.ColumnSpan="17"></Border>
                    <Border x:Name="menu_den"
                            Margin="10"
                            Background="#E8E8E8"
                            Style="{StaticResource hieu_ung_bong_den}"
                            CornerRadius="5"
                            Height="50"
                            Width="50"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Grid.Row="0"
                            Grid.Column="1"
                            Grid.RowSpan="8"
                            Grid.ColumnSpan="17"
                            MouseEnter="menu_enter"
                            MouseLeave="menu_leave"
                            MouseLeftButtonDown="menu_down">
                        <Grid Height="50"
                              Width="70">
                            <Rectangle x:Name="MyRectangle1"
                                       Margin="5"
                                       Width="40"
                                       Height="6"
                                       RadiusX="3"
                                       RadiusY="3"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0"
                                                         EndPoint="1,1">
                                        <GradientStop Color="#af40ff"
                                                      Offset="0.0" />
                                        <GradientStop Color="#5b42f3"
                                                      Offset="1.0" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                                <Rectangle.RenderTransform>
                                    <!-- Transform dùng để xoay -->
                                    <RotateTransform x:Name="RectangleRotateTransform1"
                                                     CenterX="3"
                                                     CenterY="3" />
                                </Rectangle.RenderTransform>
                            </Rectangle>

                            <Rectangle x:Name="MyRectangle2"
                                       Margin=" 5 22"
                                       Width="40"
                                       Height="6"
                                       RadiusX="3"
                                       RadiusY="3"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="center">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0"
                                                         EndPoint="1,1">
                                        <GradientStop Color="#af40ff"
                                                      Offset="0.0" />
                                        <GradientStop Color="#5b42f3"
                                                      Offset="1.0" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                                <Rectangle.RenderTransform>
                                    <!-- Transform dùng để xoay -->
                                    <RotateTransform x:Name="RectangleRotateTransform2"
                                                     CenterX="3"
                                                     CenterY="3" />
                                </Rectangle.RenderTransform>
                            </Rectangle>
                            <Rectangle x:Name="MyRectangle3"
                                       Margin=" 5"
                                       Width="40"
                                       Height="6"
                                       RadiusX="3"
                                       RadiusY="3"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Bottom">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0"
                                                         EndPoint="1,1">
                                        <GradientStop Color="#af40ff"
                                                      Offset="0.0" />
                                        <GradientStop Color="#5b42f3"
                                                      Offset="1.0" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                                <Rectangle.RenderTransform>
                                    <!-- Transform dùng để xoay -->
                                    <RotateTransform x:Name="RectangleRotateTransform3"
                                                     CenterX="3"
                                                     CenterY="3" />
                                </Rectangle.RenderTransform>
                            </Rectangle>

                        </Grid>

                    </Border>

                    <Grid HorizontalAlignment="Center"
                          VerticalAlignment="Top">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>

                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Border x:Name="seach_trang"
                                Style="{StaticResource hieu_ung_bong_trang}"
                                Margin="10"
                                Background="White"
                                CornerRadius="25"
                                HorizontalAlignment="Center"
                                Width="500"
                                Height="50"
                                VerticalAlignment="Top"></Border>
                        <Border x:Name="searh_den"
                                Margin="10"
                                Style="{StaticResource hieu_ung_bong_den}"
                                Background="White"
                                CornerRadius="25"
                                HorizontalAlignment="Center"
                                Width="500"
                                Height="50"
                                VerticalAlignment="Top">
                            <!-- Thanh tìm kiếm -->
                            <TextBox x:Name="SearchBox"
                                     FontSize="18"
                                     HorizontalContentAlignment="Left"
                                     VerticalContentAlignment="Center"
                                     Margin="20 10 10 10"
                                     KeyDown="SearchBox_KeyDown"
                                     TextChanged="SearchBox_TextChanged"
                                     Background="Transparent"
                                     BorderBrush="Transparent"
                                     BorderThickness="0" />
                        </Border>
                        <!-- Danh sách gợi ý -->


                        <!-- Nút tìm kiếm -->
                        <Border x:Name="btn_seach_trang"
                                Margin="10"
                                Background="White"
                                Style="{StaticResource hieu_ung_bong_trang}"
                                CornerRadius="25"
                                Height="50"
                                Width="50"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Grid.Column="1"></Border>
                        <Border x:Name="btn_seach_den"
                                Margin="10"
                                Background="White"
                                Style="{StaticResource hieu_ung_bong_den}"
                                CornerRadius="25"
                                Height="50"
                                Width="50"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Grid.Column="1"
                                MouseEnter="btn_seach_enter"
                                MouseLeave="btn_seach_leave"
                                MouseLeftButtonDown="btn_seach_down">
                            <Image Name="btn_seach_png"
                                   Height="30"
                                   Width="30"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center">
                                <Image.Source>
                                    <BitmapImage UriSource="png/seach_1.png" />
                                </Image.Source>
                            </Image>
                        </Border>

                        <Border x:Name="list_seach_trang"
                                Grid.Row="1"
                                Margin="10"
                                Background="#E8E8E8"
                                Style="{StaticResource hieu_ung_bong_trang}"
                                CornerRadius="25"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Grid.Column="0">
                            <Border x:Name="list_seach_den"
                                    Background="#E8E8E8"
                                    Style="{StaticResource hieu_ung_bong_den}"
                                    CornerRadius="25"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Grid.Column="0"
                                    MouseEnter="btn_seach_enter"
                                    MouseLeave="btn_seach_leave"
                                    MouseLeftButtonDown="btn_seach_down">
                                <ListBox x:Name="SuggestionList"
                                         BorderBrush="Transparent"
                                         Background="Transparent"
                                         Margin="10"
                                         Width="485"
                                         Visibility="Collapsed"
                                         SelectionChanged="SuggestionList_SelectionChanged">
                                </ListBox>
                            </Border>
                        </Border>


                    </Grid>
                    <Border x:Name="btn_nguoi_dung_trang"
                            Margin="0 10 20 0"
                            Background="White"
                            Style="{StaticResource hieu_ung_bong_trang}"
                            CornerRadius="25"
                            Height="50"
                            Width="50"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Top"
                            Grid.Column="1">
                        <Border x:Name="btn_nguoi_dung_den"
                                Background="White"
                                Style="{StaticResource hieu_ung_bong_den}"
                                CornerRadius="25"
                                Height="50"
                                Width="50"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Grid.Column="1"
                                MouseEnter="btn_nguoi_dung_enter"
                                MouseLeave="btn_nguoi_dung_leave"
                                MouseLeftButtonDown="btn_nguoi_dung_down">
                            <Image Name="btn_nguoi_dung_png"
                                   Height="30"
                                   Width="30"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center">
                                <Image.Source>
                                    <BitmapImage UriSource="png/nguoi_dung_1.png" />
                                </Image.Source>
                            </Image>
                        </Border>

                    </Border>
                    <Border x:Name="menu_nguoi_dung_trang"
                            Visibility="Collapsed"
                            Margin="0 70 20 0"
                            Height="auto"
                            Width="auto"
                            Background="#E8E8E8"
                            Style="{StaticResource hieu_ung_bong_trang}"
                            CornerRadius="25"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Top"
                            Grid.Column="1">
                        <Border x:Name="menu_nguoi_dung_den"
                                Height="auto"
                                Width="auto"
                                Background="#E8E8E8"
                                Style="{StaticResource hieu_ung_bong_den}"
                                CornerRadius="25"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Grid.Column="1">

                            <StackPanel>
                                <Border x:Name="thong_tin_trang"
                                        Margin="10"
                                        BorderBrush="#aeaeae"
                                        BorderThickness="1"
                                        CornerRadius="25"
                                        Height="50"
                                        Width="200"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Grid.Column="1">
                                    <Border x:Name="thong_tin_den"
                                            BorderBrush="#aeaeae"
                                            CornerRadius="25"
                                            Height="50"
                                            Width="200"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Grid.Column="1"
                                            MouseLeftButtonDown="thong_tin_down">
                                        <TextBlock Text="Thông tin"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="20"></TextBlock>
                                    </Border>

                                </Border>
                                
                                    <Border x:Name="lich_su_trang"
                                            Margin="10"
                                            BorderBrush="#aeaeae"
                                            BorderThickness="1"
                                            CornerRadius="25"
                                            Height="50"
                                            Width="200"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Grid.Column="1">
                                        <Border x:Name="lich_su_den"
                                                BorderBrush="#aeaeae"
                                                CornerRadius="25"
                                                Height="50"
                                                Width="200"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Grid.Column="1"
                                                MouseLeftButtonDown="lich_su_down">
                                            <TextBlock Text="Lịch sử tương tác"
                                                       HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       FontSize="20"></TextBlock>
                                        </Border>

                                    </Border>
                                    <Border x:Name="admin_trang"
                                            Margin="10"
                                            BorderBrush="#aeaeae"
                                            BorderThickness="1"
                                            CornerRadius="25"
                                            Height="50"
                                        Width="200"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Grid.Column="1">
                                        <Border x:Name="admin_den"
                                                BorderBrush="#aeaeae"
                                                CornerRadius="25"
                                                Height="50"
                                            Width="200"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Grid.Column="1"
                                                MouseLeftButtonDown="admin_down">
                                            <TextBlock Text="Super SU"
                                                       HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       FontSize="20"></TextBlock>
                                        </Border>

                                    </Border>
                                    <Border x:Name="dang_xuat_trang"
                                            Margin="10"
                                            BorderBrush="#aeaeae"
                                            BorderThickness="1"
                                            CornerRadius="25"
                                            Height="50"
                                        Width="200"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Grid.Column="1">
                                        <Border x:Name="dang_xuat_den"
                                                BorderBrush="#aeaeae"
                                                BorderThickness="1"
                                                CornerRadius="25"
                                                Height="50"
                                            Width="200"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Grid.Column="1"
                                                MouseLeftButtonDown="dang_xuat_down">
                                            <TextBlock Text="Đăng xuất"
                                                       HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       FontSize="20"></TextBlock>
                                        </Border>

                                    </Border>
                                </StackPanel>
                        </Border>

                    </Border>

                </Grid>
            </Border>

        </Grid>
    </Grid>
</Window>

